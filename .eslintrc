{
    "rules": {
        "http://eslint.org/docs/rules/#possible-errors": 0,
        "comma-dangle":                 [2, "never"],
        "no-cond-assign":               2,
        "no-console":                   0,
        "no-constant-condition":        2,
        "no-control-regex":             2,
        "no-debugger":                  2,


        "comma-style":                  [2, "last"],

        "block-scoped-var":             0,
        "camelcase":                    2,
        "comma-style":                  [2, "last"],
        "complexity":                   [2, 6],
        "curly":                        2,
        "dot-notation":                 2,
        "eqeqeq":                       2,
        "guard-for-in":                 2,
        "indent":                       [2, 4],
        "linebreak-style":              [2, "windows"],
        "max-depth":                    [2, 4],
        "max-len":                      [2, 120],
        "max-params":                   [2, 3],
        "max-statements":               [2, 15],
        "new-cap":                      2,
        "no-bitwise":                   0,
        "no-caller":                    2,
        "no-empty":                     2,
        "no-eq-null":                   0,
        "no-eval":                      2,
        "no-invalid-this":              2,
        "no-iterator":                  0,
        "no-loop-func":                 2,
        "no-multi-str":                 0,
        "no-new":                       2,
        "no-new-func":                  2,
        "no-new-wrappers":              2,
        "no-plusplus":                  0,
        "no-proto":                     0,
        "no-script-url":                0,
        "no-shadow":                    2,
        "no-undef":                     2,
        "no-unused-expressions":        0,
        "no-unused-vars":               [2, {"vars": "all", "args": "after-used"}],
        "no-use-before-define":         2,
        "quotes":                       [2, "double"],
        "semi":                         [2, "always"],
        "strict":                       0,
        "wrap-iife":                    [ 2, "any" ]
    },
    "env": {
        "node":     true,
        "browser":  true
    },
    "globals": {
        "gpf":      false
    },
    "extends": "eslint:recommended"
}
