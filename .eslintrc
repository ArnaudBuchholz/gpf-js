{
    "rules": {
        "http://eslint.org/docs/rules/#possible-errors": 0,
        "comma-dangle":                 [2, "never"],
        "no-cond-assign":               2,
        "no-console":                   0,                                     "no-console: part of the exposed API": 0,
        "no-constant-condition":        2,
        "no-control-regex":             2,
        "no-debugger":                  2,
        "no-dupe-args":                 2,
        "no-dupe-keys":                 2,
        "no-empty-character-class":     2,
        "no-empty":                     2,
        "no-ex-assign":                 2,
        "no-extra-boolean-cast":        2,
        "no-extra-parens":              2,
        "no-extra-semi":                2,
        "no-func-assign":               2,
        "no-inner-declarations":        2,
        "no-invalid-regexp":            2,
        "no-irregular-whitespace":      2,
        "no-negated-in-lhs":            2,
        "no-obj-calls":                 2,
        "no-regex-spaces":              2,
        "no-sparse-arrays":             2,
        "no-unexpected-multiline":      2,
        "no-unreachable":               2,
        "use-isnan":                    2,
        "valid-jsdoc":                  0,                                "valid-jsdoc: not eveything is documented": 0,
        "valid-typeof":                 2,

        "http://eslint.org/docs/rules/#best-practices": 0,
        "accessor-pairs":               2,
        "block-scoped-var":             2,
        "complexity":                   [2, 6],
        "consistent-return":            0,           "consistent-return: the API commonly handles undefined returns": 0,
        "curly":                        [2, "all"],
        "default-case":                 2,
        "dot-location":                 [2, "property"],
        "dot-notation":                 [2, {"allowKeywords": false}],
        "eqeqeq":                       2,
        "guard-for-in":                 2,
        "no-alert":                     2,
        "no-caller":                    2,
        "no-div-regex":                 2,
        "no-else-return":               2,
        "no-empty-label":               2,
        "no-eq-null":                   2,
        "no-eval":                      2,
        "no-extend-native":             0,                             "no-extend-native: GPF-JS provides polyfills": 0,
        "no-extra-bind":                2,
        "no-fallthrough":               2,
        "no-floating-decimal":          2,
        "no-implicit-coercion":         2,
        "no-implied-eval":              2,
        "no-invalid-this":              2,
        "no-iterator":                  2,
        "no-labels":                    2,                                               "no-labels: dont't need it": 0,
        "no-lone-blocks":               2,
        "no-loop-func":                 2,
        "no-magic-numbers":             0,                              "no-magic-numbers: not sure about the value": 0,
        "no-multi-spaces":              0,               "no-multi-spaces: I need to check how to leverage this one": 0,
        "no-multi-str":                 2,
        "no-native-reassign":           2,
        "no-new-func":                  2,
        "no-new-wrappers":              2,
        "no-new":                       2,
        "no-octal-escape":              2,
        "no-octal":                     2,
        "no-param-reassign":            0,                                  "no-param-reassign: maybe in the future": 0,
        "no-process-env":               2,
        "no-proto":                     2,
        "no-redeclare":                 2,
        "no-return-assign":             2,
        "no-script-url":                2,
        "no-self-compare":              2,
        "no-sequences":                 2,
        "no-throw-literal":             2,
        "no-unused-expressions":        2,
        "no-useless-call":              2,
        "no-useless-concat":            2,
        "no-void":                      2,
        "no-warning-comments":          0,
        "no-with":                      2,
        "radix":                        2,
        "vars-on-top":                  0,                                    "vars-on-top: I am OK if not enforced": 0,
        "wrap-iife":                    [2, "outside"],
        "yoda":                         0,                                     "yoda: do or do not, there is no try": 0,

        "http://eslint.org/docs/rules/#strict-mode": 0,
        "strict":                       [2, "global"],                                  "strict: for GPF-JS modules": 0,

        "http://eslint.org/docs/rules/#variables": 0,

        "comma-style":                  [2, "last"],
        "camelcase":                    2,
        "comma-style":                  [2, "last"],
        "indent":                       [2, 4],
        "linebreak-style":              [2, "windows"],
        "max-depth":                    [2, 4],
        "max-len":                      [2, 120],
        "max-params":                   [2, 3],
        "max-statements":               [2, 15],
        "new-cap":                      2,
        "no-bitwise":                   0,
        "no-loop-func":                 2,
        "no-plusplus":                  0,
        "no-shadow":                    2,
        "no-undef":                     2,
        "no-unused-vars":               [2, {"vars": "all", "args": "after-used"}],
        "no-use-before-define":         2,
        "quotes":                       [2, "double"],
        "semi":                         [2, "always"],

        "Custom rules": 0,
        "no-switch":                    2,
        "no-conditional":               2
    },
    "globals": {
        "gpf":                          false,
        "gpfSourcesPath":               true,
        "console":                      true,
        "setTimeout":                   true,

        "WScript":                      false,
        "ActiveXObject":                false,
        "print":                        false,
        "load":                         false,
        "java":                         false
    },
    "extends": "eslint:recommended"
}
