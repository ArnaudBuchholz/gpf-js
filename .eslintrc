{
    "rules": {
        "comma-dangle":                 [2, "never"],
        "no-cond-assign":               2,
        "no-console":                   0,
        "no-constant-condition":        2,
        "no-control-regex":             2,
        "no-debugger":                  2,
        "no-dupe-args":                 2,
        "no-dupe-keys":                 2,
        "no-empty-character-class":     2,
        "no-empty":                     2,
        "no-ex-assign":                 2,
        "no-extra-boolean-cast":        2,
        "no-extra-parens":              2,
        "no-extra-semi":                2,
        "no-func-assign":               2,
        "no-inner-declarations":        2,
        "no-invalid-regexp":            2,
        "no-irregular-whitespace":      2,
        "no-negated-in-lhs":            2,
        "no-obj-calls":                 2,
        "no-regex-spaces":              2,
        "no-sparse-arrays":             2,
        "no-unexpected-multiline":      2,
        "no-unreachable":               2,
        "use-isnan":                    2,
        "valid-jsdoc":                  [2, {"prefer": {
                                                 "returns": "return",
                                                 "class": "constructor",
                                                 "augments": "extends"
                                             },
                                             "preferType": {
                                                 "object": "Object",
                                                 "boolean": "Boolean",
                                                 "number": "Number",
                                                 "string": "String",
                                                 "function": "Function"
                                             },
                                             "requireReturn": false,
                                             "requireReturnType": true,
                                             "requireParamDescription": true,
                                             "requireReturnDescription": true
                                            }],
        "valid-typeof":                 2,
        "accessor-pairs":               2,
        "block-scoped-var":             2,
        "complexity":                   [2, 6],
        "consistent-return":            0,           "consistent-return: the API commonly handles undefined returns": 0,
        "curly":                        [2, "all"],
        "default-case":                 2,
        "dot-location":                 [2, "property"],
        "dot-notation":                 [2, {"allowKeywords": false}],
        "eqeqeq":                       2,
        "guard-for-in":                 2,
        "no-alert":                     2,
        "no-caller":                    2,
        "no-div-regex":                 2,
        "no-else-return":               2,
        "no-eq-null":                   2,
        "no-eval":                      2,
        "no-extend-native":             0,                             "no-extend-native: GPF-JS provides polyfills": 0,
        "no-extra-bind":                2,
        "no-fallthrough":               2,
        "no-floating-decimal":          2,
        "no-implicit-coercion":         2,
        "no-implied-eval":              2,
        "no-invalid-this":              2,
        "no-iterator":                  2,
        "no-labels":                    2,                                               "no-labels: dont't need it": 0,
        "no-lone-blocks":               2,
        "no-loop-func":                 2,
        "no-magic-numbers":             0,                              "no-magic-numbers: not sure about the value": 0,
        "no-multi-spaces":              0,               "no-multi-spaces: I need to check how to leverage this one": 0,
        "no-multi-str":                 2,
        "no-native-reassign":           2,
        "no-new-func":                  2,
        "no-new-wrappers":              2,
        "no-new":                       2,
        "no-octal-escape":              2,
        "no-octal":                     2,
        "no-param-reassign":            0,                                  "no-param-reassign: maybe in the future": 0,
        "no-process-env":               2,
        "no-proto":                     2,
        "no-redeclare":                 2,
        "no-return-assign":             2,
        "no-script-url":                2,
        "no-self-compare":              2,
        "no-sequences":                 2,
        "no-throw-literal":             2,
        "no-unused-expressions":        2,
        "no-useless-call":              2,
        "no-useless-concat":            2,
        "no-void":                      2,
        "no-warning-comments":          0,
        "no-with":                      2,
        "radix":                        2,
        "vars-on-top":                  0,                                    "vars-on-top: I am OK if not enforced": 0,
        "wrap-iife":                    [2, "outside"],
        "yoda":                         0,                                     "yoda: do or do not, there is no try": 0,

        "strict":                       [2, "global"],                                  "strict: for GPF-JS modules": 0,

        "init-declarations":            0,                             "init-declarations: as GPF-JS uses undefined": 0,
        "no-catch-shadow":              2,
        "no-delete-var":                2,
        "no-label-var":                 2,
        "no-shadow-restricted-names":   2,
        "no-shadow":                    2,
        "no-undef-init":                2,
        "no-undef":                     2,
        "no-undefined":                 0,                                  "no-undefined: as GPF-JS uses undefined": 0,
        "no-unused-vars":               [2, {"vars": "all", "args": "after-used"}],
        "no-use-before-define":         [2, "nofunc"],

        "callback-return":              0,
        "global-require":               0,
        "handle-callback-err":          2,
        "no-mixed-requires":            0,
        "no-new-require":               2,
        "no-path-concat":               0,            "no-path-concat: GPF-JS handles path separator the proper way": 0,
        "no-process-exit":              0,
        "no-restricted-modules":        0,
        "no-sync":                      2,

        "array-bracket-spacing":        [2, "never"],
        "block-spacing":                [2, "never"],
        "brace-style":                  [2, "1tbs"],
        "camelcase":                    2,
        "comma-spacing":                [2, {"before": false, "after": true}],
        "comma-style":                  [2, "last"],
        "computed-property-spacing": 0,
        "consistent-this":              [2, "me"],
        "eol-last":                     2,
        "func-names":                   0,
        "func-style":                   [2, "declaration"],                        "func-style: enforced on purpose": 0,
        "id-length":                    0,
        "id-match":                     0,                           "id-match: will be replaced with a custom rule": 0,
        "indent":                       [2, 4],
        "jsx-quotes":                   0,
        "key-spacing":                  [2, {"beforeColon": false, "afterColon": true, "mode": "minimum"}],
        "linebreak-style":              [1, "unix"],
        "lines-around-comment":         0,
        "max-nested-callbacks":         [2, 2],
        "new-cap":                      2,
        "new-parens":                   2,
        "newline-after-var":            0,
        "no-array-constructor":         2,
        "no-continue":                  0,
        "no-inline-comments":           0,
        "no-lonely-if":                 2,
        "no-mixed-spaces-and-tabs":     2,
        "no-multiple-empty-lines":      2,
        "no-negated-condition":         2,
        "no-nested-ternary":            2,
        "no-new-object":                2,
        "no-restricted-syntax":         [2, "SwitchStatement"],
        "no-spaced-func":               2,
        "no-ternary":                   2,
        "no-trailing-spaces":           2,
        "no-underscore-dangle":         0,
        "no-unneeded-ternary":          2,
        "object-curly-spacing":         [2, "never"],
        "one-var":                      0,                  "one-var: variables are declared with related functions": 0,
        "operator-assignment":          [2, "always"],
        "operator-linebreak":           [2, "before"],
        "padded-blocks":                0,
        "quote-props":                  [1, "as-needed", {"keywords": true, "unnecessary": false}],
        "quotes":                       [2, "double"],
        "require-jsdoc":                0,
        "semi-spacing":                 [2, {"before": false, "after": true}],
        "semi":                         [2, "always"],
        "sort-vars":                    0,
        "space-before-blocks":          [2, {"functions": "always", "keywords": "always"}],
        "space-before-function-paren":  [2, "always"],
        "keyword-spacing":              [2, {"before": true, "after": true}],
        "space-in-parens":              [2, "never"],
        "space-infix-ops":              [2, {"int32Hint": false}],
        "space-unary-ops":              2,
        "spaced-comment":               0,
        "wrap-regex":                   2,

        "arrow-parens":                 0,
        "arrow-spacing":                0,
        "constructor-super":            0,
        "generator-star-spacing":       0,
        "no-class-assign":              0,
        "no-const-assign":              0,
        "no-dupe-class-members":        0,
        "no-this-before-super":         0,
        "no-var":                       0,
        "object-shorthand":             0,
        "prefer-arrow-callback":        0,
        "prefer-const":                 0,
        "prefer-reflect":               0,
        "prefer-spread":                0,
        "prefer-template":              0,
        "require-yield":                0,

        "max-depth":                    [2, 4],
        "max-len":                      [2, 120],
        "max-params":                   [2, 3],
        "max-statements":               [2, 15],
        "no-bitwise":                   0,
        "no-plusplus":                  0
    },
    "globals": {
        "gpf":                          false,
        "gpfSourcesPath":               true,
        "console":                      true,
        "setTimeout":                   true,
        "Promise":                      true,

        "WScript":                      false,
        "ActiveXObject":                false,
        "print":                        false,
        "load":                         false,
        "readFile":                     false,
        "java":                         false
    },
    "extends": "eslint:recommended"
}
