{
    "output": "gpf",
    "uglify": true,
    "saveTemporaryfiles": true,
    "define": {
        "UMD": true,
        "DEBUG": false
    },
    "optimize": true,
    "rewriteOptions": {
        "format": {
            "indent": {
                "style": "",
                "base": 0,
                "adjustMultilineComment": false
            },
            "newline": "",
            "space": "",
            "json": false,
            "renumber": false,
            "hexadecimal": false,
            "quotes": "double",
            "escapeless": false,
            "reduce": true,
            "parentheses": false,
            "semicolons": true,
            "safeConcatenation": true
        },
        "comment": false
    }
}
