{
    "saveTemporaryfiles": true,
    "define": {
        "UMD": true,
        "DEBUG": true
    },
    "reduce": false,
    "rewriteOptions": {
        "format": {
            "indent": {
                "style": "    ",
                "base": 0,
                "adjustMultilineComment": false
            },
            "newline": "\n",
            "space": " ",
            "json": false,
            "renumber": false,
            "hexadecimal": false,
            "quotes": "double",
            "escapeless": false,
            "reduce": false,
            "parentheses": true,
            "semicolons": true,
            "safeConcatenation": false
        },
        "comment": true
    }
}
